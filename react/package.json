{
  "name": "react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:ssr:dev": "nodemon src/server.ts",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts --ext .tsx",
    "lint:fix": "eslint --fix . --ext .ts --ext .tsx",
    "format": "prettier --write .",
    "test": "jest --watchAll=false --coverage  --collectCoverageFrom=src/**/*.{ts,tsx} --detectOpenHandles --forceExit",
    "test:e2e:terminal": "start-server-and-test start:ssr:dev http-get://localhost:3333 cy:e2e:terminal",
    "test:e2e:browser": "start-server-and-test start:ssr:dev http-get://localhost:3333 cy:e2e:browser",
    "cy:e2e:terminal": "npx cypress run --e2e && npx nyc report --reporter=text-summary",
    "cy:e2e:browser": "npx cypress open --e2e --browser chrome && npx nyc report --reporter=text-summary"
  },
  "dependencies": {
    "@babel/core": "^7.21.3",
    "@jest/globals": "^29.5.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/giphy-api": "^2.0.3",
    "@types/uuid": "^9.0.1",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-css": "^6.0.1",
    "msw": "^1.2.1",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0",
    "test-jest": "^1.0.1",
    "ts-jest": "^29.0.5",
    "uuid": "^9.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^3.1.0",
    "cypress": "^12.11.0",
    "cypress-vite": "^1.4.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-istanbul": "^4.0.1"
  }
}
